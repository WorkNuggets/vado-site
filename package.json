{
  "name": "vado-site",
  "version": "1.0.0",
  "private": true,
  "license": "(ISC) SEE LICENSE IN LICENSE.md",
  "description": "This is a repo for the landing page website of our startup VADO - Home Touring. Made with the Astrowind 4.0 template",
  "type": "module",
  "author": {
    "name": "Spenser Wellen",
    "email": "spenserwellen+githubvadosite@gmail.com",
    "url": "https://spenserwellen.com"
  },
  "contributors": [
    {
      "name": "John Shotton",
      "email": "johnshotton+githubvadosite@gmail.com",
      "url": "https://johnshotton.com"
    }
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "predeploy": "pnpm build",
    "preview": "astro preview",
    "render": "pnpm build && pnpm preview",
    "astro": "astro",
    "serve": "next start",
    "prenuke": "rm -rf .next node_modules",
    "nuke": "pnpm i",
    "postnuke": "pnpm start",
    "restart": "pnpm nuke",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:css": "stylelint 'src/**/*.{css,scss}'",
    "lint:css:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "check-env": "node -e 'console.log(process.env)' | grep pnpm",
    "check": "pnpm run check:astro && pnpm run check:eslint && pnpm run check:prettier",
    "check:astro": "astro check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier -w ."
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@fontsource-variable/inter": "^5.1.0",
    "astro": "^5.4.2",
    "astro-embed": "^0.7.4",
    "astro-icon": "^1.1.1",
    "comment-json": "^4.2.5",
    "limax": "4.1.0",
    "lodash.merge": "^4.6.2",
    "sweetalert2": "^11.14.4",
    "unpic": "^3.18.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.1.0",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/tailwind": "6.0.0",
    "@eslint/js": "^9.21.0",
    "@iconify-json/flat-color-icons": "^1.2.0",
    "@iconify-json/tabler": "^1.2.5",
    "@playwright/test": "^1.51.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint__js": "^8.42.3",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/mdx": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "astro-compress": "2.3.3",
    "astro-eslint-parser": "^1.2.1",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^15.15.0",
    "js-yaml": "^4.1.0",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "reading-time": "^1.5.0",
    "sharp": "0.33.5",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "engines": {
    "pnpm": ">=9.12.3",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "node": ">=20.0.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
